Algoritmo "par,impar"

Var
par:inteiro
impar:inteiro
num:inteiro
Inicio
escreval("Digite um número: ")
leia(num)

num <- (num)

se num mod 2 = 0 entao
  par <- (num + 5)
  escreval("A soma desse numero é",par)
senao
    impar <- (num + 8)
      escreval("A soma desse numero é",impar)
fimse
Fimalgoritmo
